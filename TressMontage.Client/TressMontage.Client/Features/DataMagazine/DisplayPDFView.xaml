<?xml version="1.0" encoding="utf-8"?>
<local:DisplayPDFViewBase xmlns="http://xamarin.com/schemas/2014/forms" 
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	xmlns:local="clr-namespace:TressMontage.Client.Features.DataMagazine;assembly=TressMontage.Client"
	x:Class="TressMontage.Client.Features.DataMagazine.DisplayPDFView">
    <Grid
        Style="{StaticResource FormsGrid}">
		<local:CustomWebView 
			Uri="{Binding PDFDirectory}" 
			HorizontalOptions="FillAndExpand" 
			VerticalOptions="FillAndExpand" />
	</Grid>
</local:DisplayPDFViewBase>
